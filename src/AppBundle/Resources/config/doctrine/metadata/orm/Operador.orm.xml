<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Operador" table="operador">
    <id name="cod" type="integer" column="cod">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="senha" type="string" column="senha" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
